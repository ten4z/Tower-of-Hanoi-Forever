- const isTemplate = self.htmlWebpackPlugin.options.isTemplate

doctype html
html
  head
    title Blend4Web Player
    meta(property="og:title", content="Blend4Web Player")
    meta(charset="utf-8")
    meta(name="viewport", content="initial-scale=1.0, user-scalable=no, width=device-width")
    meta(name="description", content="Check out these awesome 3D graphics made with Blend4Web!")
    meta(name="og:description", content="Check out these awesome 3D graphics made with Blend4Web!")
    meta(property="og:image", content="data:image/png;base64,")
    link(rel="shortcut icon", href="data:image/png;base64,")
    link(rel="image_src", href="data:image/png;base64,")
    if isTemplate
      |$b4w_meta
      style(type="text/css") $styles
      script(type="text/javascript", id="built_in_scripts") $scripts; b4w.module["built_in_data"] = function(exports, require) {exports["data"] = $built_in_data}
  body
    div#main_canvas_cont
    div#preloader_cont.active
      div#bg
      div#logo_cont
        svg
          circle(id="svg_preloader_circle",
                 shape-rendering="geometricPrecision",
                 r="54",
                 cx="60",
                 fill="transparent",
                 cy="60",
                 transform="rotate(-90 60 60)",
                 stroke-width="4px")
    div#ctrl_wrap
      div#ctrl_scroll
        div#timeline_cont
          div#timeline
            div#line
          div#pointer
        div#ctrl_cont
          div#soc_cont
            div#soc_btn.ctrl_btn
              img(inline src="src/img/soc_btn.svg")
          div#play_cont
            div#play_btn.ctrl_btn.on
              div.im2
                img(inline src="src/img/play_btn.svg")
              div.im1
                img(inline src="src/img/pause_btn.svg")
            div#time_cont
          div#btns_cont
            div#stereo_btn.ctrl_btn
              div.none
                img(inline src="src/img/default.svg")
              div.anaglyph
                img(inline src="src/img/anaglyph.svg")
              div.sidebyside
                img(inline src="src/img/sidebyside.svg")
              div.hmd
                img(inline src="src/img/vr.svg")
            div#sound_btn.ctrl_btn.on
              div.im1
                img(inline src="src/img/sound_on_btn.svg")
              div.im2
                img(inline src="src/img/sound_off_btn.svg")
            div#auto_rotate_btn.ctrl_btn
              div.im2
                img(inline src="src/img/rotate_on_btn.svg")
              div.im1
                img(inline src="src/img/rotate_off_btn.svg")
            div#quality_btn.ctrl_btn
              div.low
                img(inline src="src/img/quality_low.svg")
              div.high
                img(inline src="src/img/quality_high.svg")
              div.ultra
                img(inline src="src/img/quality_ultra.svg")
            div#help_btn.ctrl_btn
              img(inline src="src/img/help_btn.svg")
            div#fullscreen_btn.ctrl_btn
              div.im2
                img(inline src="src/img/fs_on_btn.svg")
              div.im1
                img(inline src="src/img/fs_off_btn.svg")
          div#timer
          div#tooltip_cont
            div#fs_on_tooltip.tooltip
            div#fs_off_tooltip.tooltip
            div#autorotate_on_tooltip.tooltip
            div#autorotate_off_tooltip.tooltip
            div#sound_on_tooltip.tooltip
            div#sound_off_tooltip.tooltip
            div#play_tooltip.tooltip
            div#pause_tooltip.tooltip
    div#social.long
      a(target="_blank", href="//twitter.com/intent/tweet?via=blend4web&text=Check out these awesome 3D graphics made with Blend4Web!&url=")
        div.item#tw
          img(inline src="src/img/tw.svg")
      a(target="_blank", href="https://vk.com/share.php?url=")
        div.item#vk
          img(inline src="src/img/vk.svg") 
      a(target="_blank", href="https://www.facebook.com/sharer/sharer.php?u=")
        div.item#fb
          img(inline src="src/img/fb.svg")
      a(target="_blank", href="https://plus.google.com/share?url=")
        div.item#gplus
          img(inline src="src/img/gplus.svg")
      a(target="_blank", href="http://service.weibo.com/share/share.php?title=看看这些用 Blend4Web 所制作的惊人 3D 图形！&url=")
        div.item#weibo
          img(inline src="src/img/weibo.svg")
      a(target="_blank", href="http://tumblr.com/widgets/share/tool?canonicalUrl=")
        div.item#tumblr
          img(inline src="src/img/tumblr.svg")
      a(target="_blank", href="http://www.reddit.com/submit?url=")
        div.item#reddit
          img(inline src="src/img/reddit.svg")
    div#stereo
        div.item#none
          svg(width="40", height="40", viewBox="0 0 40 40")
            use(xlink:href="#_svg_none_btn")
          div Default
        div.item#hmd
          svg(width="40", height="40", viewBox="0 0 40 40")
            use(xlink:href="#_svg_vr_btn")
          div VR
        div.item#anaglyph
          svg(width="40", height="40", viewBox="0 0 40 40")
            use(xlink:href="#_svg_anaglyph_btn")
          div Anaglyph
        div.item#sidebyside
          svg(width="40", height="40", viewBox="0 0 40 40")
            use(xlink:href="#_svg_sidebyside_btn")
          div Side-by-Side
    div#quality
      div.item#low
        svg(width="40", height="40", viewBox="0 0 40 40")
          use(xlink:href="#_svg_q_low_btn")
        div Low
      div.item#high
        svg(width="40", height="40", viewBox="0 0 40 40")
          use(xlink:href="#_svg_q_high_btn")
        div High
      div.item#ultra
        svg(width="40", height="40", viewBox="0 0 40 40")
          use(xlink:href="#_svg_q_ultra_btn")
        div Ultra
    div#help_cont
      div#close_help
      div#logo_lang
        div.lang#ru Rus
          .border
        div.lang.active#en Eng
          .border
      div#logo_help
      div#rel_version
      div.content
        div.head
          div.cam_type
        div#help_body.body  
        //-
          div.wrap.ru.mobile.static
            div.help_entry
               p Данный тип камеры не может контролироваться пользователем. Это — программируемый тип камеры.

          div.wrap.ru.mobile.hover
            div.row
              p изменение угла наклона камеры относительно горизонтальной плоскости
              p поворот камеры вокруг вертикальной оси (Z)
              p перемещение камеры в горизонтальном или вертикальном направлении
              p взаимодействие с интерактивными элементами
          div.wrap.ru.mobile.eye
            div.row
              p вращение камеры относительно собственного центра
              p взаимодействие с интерактивными элементами
          div.wrap.ru.mobile.target
            div.row
              p приближение и отдаление относительно цели (наезд/отъезд)
              p вращение камеры относительно цели в любом направлении
              p взаимодействие с интерактивными элементами
          div.wrap.en.mobile.static
            div.row
              p this type of camera can not be controlled by the user. This is a programmable type of  camera.
          div.wrap.en.mobile.hover
            div.row
              p the change in the angle of inclination of the chamber relative to the horizontal plane
              p rotate the camera around the vertical axis (Z)
              p moving the camera horizontally or vertically
              p interaction with interactive elements
          div.wrap.en.mobile.eye
            div.row
              p rotation of the camera relative to its own center
              p interaction with interactive elements
          
          div.wrap.en.mobile.target
            div#help_target_interact.help_entry
              div
              p approach and distance from the target (hitting / leaving)
              p rotate the camera relative to the target in any direction
              p interaction with interactive elements
    div#error_cont
      div#error_name
      div#error_info
    div#fps_cont
      div#prev_fps_circle
      div#fps_cont